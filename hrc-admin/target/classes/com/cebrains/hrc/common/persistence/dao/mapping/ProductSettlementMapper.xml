        <result column="payment_method" property="paymentMethod" />
        <result column="member" property="member" />
        <result column="membership_card" property="membershipCard" />
        <result column="payment_amount" property="paymentAmount" />
        <result column="status" property="status" />
        <result column="create_time" property="createTime" />
        <result column="flag" property="flag" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, product, payment_method AS paymentMethod, member,membership_card AS membershipCard, payment_amount AS paymentAmount, status, create_time AS createTime, flag
    </sql>

</mapper>
